{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "no-magic-numbers": true,
        "no-console": [
            true,
            "log"
        ],
        "no-var-keyword": true,
        "indent": [
            true,
            "spaces",
            4
        ],
        "max-classes-per-file": [
            true,
            2
        ],
        "max-line-length": [
            true,
            120
        ],
        "no-require-imports": true,
        "arrow-return-shorthand": true,
        "class-name": true,
        "encoding": true,
        "interface-name": [
            true,
            "always-prefix"
        ],
        "no-consecutive-blank-lines": true
    },
    "rulesDirectory": []
}